<van-tabbar
  active="{{ active }}"
  bind:change="onChange"
>
  <van-tabbar-item  wx:for="{{ list }}" wx:key="index" inactive-color="#616974" active-color="#2582CB">
    <image
      slot="icon"
      src="{{item.icon}}"
      mode="aspectFit"
      style="width: 30px; height: 18px;"
    />
    <image
      slot="icon-active"
      src="{{item.iconSelected}}"
      mode="aspectFit"
      style="width: 30px; height: 18px;"
    />
    {{item.text}}
  </van-tabbar-item>
</van-tabbar>